{
  "name": "nuxt-boilerplate",
  "description": "An opinionated starter template for Nuxt.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "storybook": "nuxt storybook",
    "storybook:build": "rm -r static/storybook && nuxt storybook build && mv storybook-static static/storybook",
    "chromatic": "npx chromatic --build-script-name=storybook:build",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest"
  },
  "dependencies": {
    "@braid/vue-formulate": "2.5.3",
    "@mapbox/mapbox-gl-geocoder": "4.7.4",
    "@nuxt/image": "0.6.2",
    "@nuxtjs/gtm": "2.4.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/robots": "2.5.0",
    "@nuxtjs/sanity": "0.10.0",
    "@nuxtjs/sitemap": "2.4.0",
    "@studiometa/vue-mapbox-gl": "1.4.7",
    "core-js": "3.22.5",
    "filesize": "8.0.7",
    "mapbox-gl": "1.13.2",
    "nuxt": "2.15.8",
    "nuxt-i18n": "6.28.1",
    "portal-vue": "2.1.7",
    "slugify": "1.6.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@etchteam/storybook-addon-status": "4.2.0",
    "@nuxtjs/eslint-config": "10.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/html-validator": "0.6.0",
    "@nuxtjs/storybook": "4.3.1",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/svg": "0.4.0",
    "@nuxtjs/tailwindcss": "4.2.1",
    "@storybook/addon-a11y": "6.5.4",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.3.4",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tailwindcss/typography": "0.4.1",
    "@vue/test-utils": "1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "28.1.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.7.1",
    "jest": "28.1.0",
    "postcss": "8.4.13",
    "postcss-easing-gradients": "3.0.1",
    "postcss-viewport-height-correction": "1.1.1",
    "prettier": "2.6.2",
    "storybook-addon-pseudo-states": "1.9.0",
    "storybook-dark-mode": "1.1.0",
    "storybook-mobile": "1.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "22.0.0",
    "tailwindcss-debug-screens": "2.2.1",
    "vue-jest": "3.0.7"
  }
}
