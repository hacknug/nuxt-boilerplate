{
  "name": "nuxt-boilerplate",
  "description": "An opinionated starter template for Nuxt.js",
  "version": "1.0.0",
  "packageManager": "yarn@1.22.22",
  "private": true,
  "scripts": {
    "dev": "nuxt --hostname '0'",
    "build": "nuxt build",
    "start": "nuxt start --hostname '0'",
    "generate": "nuxt generate",
    "storybook": "nuxt storybook",
    "storybook:build": "rm -r -f static/storybook && nuxt storybook build && mv storybook-static static/storybook",
    "chromatic": "npx chromatic --build-script-name=storybook:build",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:css": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:css",
    "test": "jest",
    "update:major": "nlx taze major -r -l -I",
    "update:minor": "nlx taze minor -r -l -I",
    "update:patch": "nlx taze patch -r -l -I"
  },
  "dependencies": {
    "@braid/vue-formulate": "2.5.3",
    "@nuxt/image": "0.7.2",
    "@nuxtjs/gtm": "2.4.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/robots": "2.5.0",
    "@nuxtjs/sanity": "0.10.0",
    "@nuxtjs/sitemap": "2.4.0",
    "core-js": "3.37.1",
    "filesize": "10.1.6",
    "nuxt": "2.17.4",
    "nuxt-i18n": "6.28.1",
    "portal-vue": "2.1.7",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.24.7",
    "@etchteam/storybook-addon-status": "4.2.2",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/html-validator": "1.8.2",
    "@nuxtjs/storybook": "4.3.2",
    "@nuxtjs/stylelint-module": "4.2.2",
    "@nuxtjs/svg": "0.4.1",
    "@nuxtjs/tailwindcss": "6.12.0",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-centered": "5.3.21",
    "@storybook/core": "6.5.16",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.13",
    "@vue/test-utils": "1.3.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "29.7.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.26.0",
    "jest": "29.7.0",
    "postcss": "8.4.38",
    "postcss-easing-gradients": "3.0.1",
    "postcss-viewport-height-correction": "1.1.1",
    "prettier": "3.5.2",
    "storybook-addon-pseudo-states": "1.15.5",
    "storybook-addon-turbo-build": "2.0.1",
    "storybook-dark-mode": "1.1.2",
    "storybook-mobile": "1.0.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "22.0.0",
    "tailwindcss-debug-screens": "2.2.1",
    "vue-jest": "3.0.7"
  }
}
